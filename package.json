{
  "name": "potential-enigma",
  "version": "1.0.0",
  "description": "A test project based on angularjs, webpack and es2015.",
  "main": "index.js",
  "scripts": {
    "test": "karma start",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "build": "rimraf dist && NODE_ENV=production node node_modules/.bin/webpack",
    "start": "npm run dev",
    "dev": "node node_modules/.bin/webpack-dev-server --devtool eval --config webpack.config.js --progress --colors --content-base app"
  },
