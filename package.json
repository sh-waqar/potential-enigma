  "bugs": {
    "url": "https://github.com/sh-waqar/potential-enigma/issues"
  },
  "homepage": "https://github.com/sh-waqar/potential-enigma#readme",
  "devDependencies": {
    "angular-mocks": "^1.5.7",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.5.0",
    "css-loader": "^0.23.1",
    "html-webpack-plugin": "^2.21.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.0.0",
